{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Error",
      "Microsoft.AspNetCore": "Error",
      "Microsoft.Hosting.Lifetime": "Warning",
      "SpreadAggregator": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5000"
      }
    }
  },
  "ConnectionStrings": {
    "WebSocket": "ws://0.0.0.0:8181"
  },
  "DataLake": {
    "Path": "./data/market_data"
  },
  "Analyzer": {
    "StatsPath": "./data/summary_stats"
  },
  "ExchangeSettings": {
    "Exchanges": {
      "Mexc": {
        "VolumeFilter": {
          "MinUsdVolume": 100000,
          "MaxUsdVolume": 999999999999
        }
      }
    }
  },
  "Recording": {
    "Enabled": false,
    "DataRootPath": "./data/market_data",
    "BatchSize": 10000
  },
  "StreamSettings": {
    "EnableTickers": false,
    "EnableTrades": true
  }
}